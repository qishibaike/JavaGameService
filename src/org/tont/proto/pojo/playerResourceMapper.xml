<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.tont.proto.pojo.PlayerResourceMapper">
    
    <select id="getAllByPid" parameterType="int" 
        resultType="org.tont.proto.pojo.PlayerResource">
        SELECT * FROM PLAYER_RES WHERE PID=#{pid}
    </select>
    
</mapper>